{
    "family": "task-definition-cfn",
    "containerDefinitions": [
        {
            "name": "sample-fargate-app",
            "image": "975050135764.dkr.ecr.ap-southeast-2.amazonaws.com/next-with-jest:xxx",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "executionRoleArn": "arn:aws:iam::975050135764:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    }
}
